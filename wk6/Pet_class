class Pet:
    name = ""
    species = ""
    age = 0
    price = 0.0


pet1 = Pet()
pet1.name = "Fluffy"
pet1.species = "Cat"
pet1.age = 3
pet1.price = 100.0
print(pet1)
print(pet1.name)

# Print pet1 object
print("-----Printing pet1 object-----")
print(pet1.__dict__)
# To update the price of pet1, you would use bracket notation, as follows.
pet1.__dict__["price"] = 120.0
print(pet1.price)
